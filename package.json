{
  "publisher": "magneet",
  "repository": "https://github.com/magnetenstad/prat-vscode",
  "name": "prat",
  "displayName": "Prat",
  "description": "Syntax highlighting for .prat files",
  "icon": "assets/prat.png",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.52.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prat",
        "aliases": [
          "prat",
          "pratjs",
          "prat.js",
          "pratpy",
          "prat.py"
        ],
        "extensions": [
          ".prat"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prat",
        "scopeName": "source.prat",
        "path": "./syntaxes/prat.tmLanguage.json"
      }
    ]
  }
}
